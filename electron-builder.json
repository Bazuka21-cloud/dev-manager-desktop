{
  "productName": "webos-dev-manager",
  "directories": {
    "output": "release/"
  },
  "files": [
    "package.json",
    "index.js",
    "main/**/*.js",
    {
      "from": "ui/dist",
      "to": "ui"
    }
  ],
  "win": {
    "icon": "ui/dist/assets/icons",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "ui/dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "ui/dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}
